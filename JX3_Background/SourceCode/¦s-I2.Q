[Script]

Function TriggerClick(chkSet_Value,cboSet_ListIndex,x,y)  
    If chkSet_Value = 1 Then 
    	If cboSet_ListIndex = 0 Then 
            Call Plugin.Bkgnd.LeftClick(Hwnd, x, y)
            Delay 10
        ElseIf cboSet_ListIndex = 1 Then 
            Call Plugin.Bkgnd.MiddleClick(Hwnd, x, y)
            Delay 10
        ElseIf cboSet_ListIndex = 2 Then
            Call Plugin.Bkgnd.RightClick(Hwnd, x, y)
            Delay 10
        ElseIf cboSet_ListIndex = 3 Then
            Call Plugin.WNDEX7_71.SendMouseWheel(Hwnd,-1)
            Delay 10
        ElseIf cboSet_ListIndex = 4 Then
            Call Plugin.WNDEX7_71.SendMouseWheel(Hwnd, 1 )
            Delay 10
        ElseIf cboSet_ListIndex = 5 Then
            Call Plugin.Bkgnd.LeftClick(Hwnd, x, y)
            Call Plugin.Bkgnd.LeftClick(Hwnd, x, y)
            Delay 10
        End If
        Delay 50
    End If
    TriggerClick = 1
End Function  

Sub PointGet
    Hwnd = Plugin.Window.MousePoint
    sRect = Plugin.Window.GetClientRect(Hwnd)
    arr_tmp = Split(sRect, "|")
    Call GetCursorPos(x, y)
    x=x-CLng(arr_tmp(0)):y=y-CLng(arr_tmp(1))
    FormMouseClick.txtLocation_X.Text = x
    FormMouseClick.txtLocation_Y.Text = y
    ExitScript
End Sub

Sub Fishing
    Hwnd = Plugin.Window.MousePoint
    Call Plugin.bgkms6_10.FakeWindowState(Hwnd, 2)
    // 獲取當前程式句柄
    x1 = FormMouseClick.txtSet_1X.Text
    y1 = FormMouseClick.txtSet_1Y.Text
    
    x2 = FormMouseClick.txtSet_2X.Text
    y2 = FormMouseClick.txtSet_2Y.Text
    
    x3 = FormMouseClick.txtSet_3X.Text
    y3 = FormMouseClick.txtSet_3Y.Text
    
    x4 = FormMouseClick.txtSet_4X.Text
    y4 = FormMouseClick.txtSet_4Y.Text

    x5 = FormMouseClick.txtSet_5X.Text
    y5 = FormMouseClick.txtSet_5Y.Text
    
    x6 = FormMouseClick.txtSet_6X.Text
    y6 = FormMouseClick.txtSet_6Y.Text

    d = FormMouseClick.txtDelay.Text// Delay Set
    
    Rem a
	res = TriggerClick(FormMouseClick.chkSet_1.Value, FormMouseClick.cboSet_1.ListIndex, x1, y1)
	res = TriggerClick(FormMouseClick.chkSet_2.Value, FormMouseClick.cboSet_2.ListIndex, x2, y2)
	res = TriggerClick(FormMouseClick.chkSet_3.Value, FormMouseClick.cboSet_3.ListIndex, x3, y3)
	res = TriggerClick(FormMouseClick.chkSet_4.Value, FormMouseClick.cboSet_4.ListIndex, x4, y4)
	res = TriggerClick(FormMouseClick.chkSet_5.Value, FormMouseClick.cboSet_5.ListIndex, x5, y5)
	res = TriggerClick(FormMouseClick.chkSet_6.Value, FormMouseClick.cboSet_6.ListIndex, x6, y6)
	
    Delay d
    Goto a
End Sub


Event FormMouseClick.btnSet_1.Click
    FormMouseClick.txtSet_1X.Text = FormMouseClick.txtLocation_X.Text
    FormMouseClick.txtSet_1Y.Text = FormMouseClick.txtLocation_Y.Text
End Event
Event FormMouseClick.btnSet_2.Click
    FormMouseClick.txtSet_2X.Text = FormMouseClick.txtLocation_X.Text
    FormMouseClick.txtSet_2Y.Text = FormMouseClick.txtLocation_Y.Text
End Event
Event FormMouseClick.btnSet_3.Click
    FormMouseClick.txtSet_3X.Text = FormMouseClick.txtLocation_X.Text
    FormMouseClick.txtSet_3Y.Text = FormMouseClick.txtLocation_Y.Text
End Event
Event FormMouseClick.btnSet_4.Click
    FormMouseClick.txtSet_4X.Text = FormMouseClick.txtLocation_X.Text
    FormMouseClick.txtSet_4Y.Text = FormMouseClick.txtLocation_Y.Text
End Event
Event FormMouseClick.btnSet_5.Click
    FormMouseClick.txtSet_5X.Text = FormMouseClick.txtLocation_X.Text
    FormMouseClick.txtSet_5Y.Text = FormMouseClick.txtLocation_Y.Text
End Event
Event FormMouseClick.btnSet_6.Click
    FormMouseClick.txtSet_6X.Text = FormMouseClick.txtLocation_X.Text
    FormMouseClick.txtSet_6Y.Text = FormMouseClick.txtLocation_Y.Text
End Event



If FormMouseClick.opbSampleMode.Value = 1 Then 
    Call PointGet 
ElseIf FormMouseClick.opbExecuteMode.Value = 1 Then
    Call Fishing
Else 
    MessageBox "Please select mode"
    ExitScript
End If
